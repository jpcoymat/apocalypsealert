<h1>Listing work_orders</h1>

<table>
  <thead>
    <tr>
      <th>Work order number</th>
      <th>Product</th>
      <th>Location</th>
      <th>Production date</th>
      <th>Quantity</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_orders.each do |work_order| %>
      <tr>
        <td><%= work_order.work_order_number %></td>
        <td><%= work_order.product_id %></td>
        <td><%= work_order.location_id %></td>
        <td><%= work_order.production_date %></td>
        <td><%= work_order.quantity %></td>
        <td><%= work_order.organization_id %></td>
        <td><%= link_to 'Show', work_order %></td>
        <td><%= link_to 'Edit', edit_work_order_path(work_order) %></td>
        <td><%= link_to 'Destroy', work_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order', new_work_order_path %>
