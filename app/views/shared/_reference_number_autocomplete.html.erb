
  <%= form.label "Line Number", {class: "col-sm-1 control-label"} %>
  <script>
    $(function() {
      var movement_sources = [
        <% movement_sources.each do |ms| %>
          "<%= ms.attributes[object_reference_number] %>",
        <% end %>
      ""];
      $( "#<%= source_type%>_<%= object_reference_number%>" ).autocomplete({
        source: movement_sources
      });
    });
  </script>
  <div class="col-sm-2"> 
    <%= form.text_field object_reference_number, class: "form-control" %>
  </div>
