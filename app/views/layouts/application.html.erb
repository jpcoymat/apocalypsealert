<!DOCTYPE html>
<html>
<head>
  <title>Apocalypsealert</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= yield :head %>
  <%= csrf_meta_tags %>
</head>
<body>
  
  <%= render partial: "shared/aggregations_menu" %>
  
  <div class="container bs-docs-container">
	  <div class="row">
			<div class="col-md-12">
				<div class="bs-docs-section">
					<% if notice %>
				    <div class="alert alert-danger" role="alert"><p><%= notice %></p></div>
				  <% end %>
			    <%= yield %>
				</div>
			</div>
	  </div>
  </div>
</body>
</html>
