<!DOCTYPE html>
<html>
<head>
  <title>Apocalypsealert</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container-fluid">
       <%= render partial: "shared/main_menu" %>
       <br \><br \>
       <% if notice %>
         <div class="alert alert-danger" role="alert"><p><%= notice %></p></div>
       <% end %>

<%= yield %>
<br \><br \>
<%= link_to "Log Out", login_logout_path, method: :post %>
  </div>
</body>
</html>
