<% if @shipment_line.errors.any? %>
  <div id="error_explanation">
    <h2>This Ship Line contains <%= pluralize(@shipment_line.errors.count, "error") %>  :</h2>
    <ul>
    <% @shipment_line.errors.each do |key,value| %>
      <li><%= key.to_s.capitalize + " " + value %></li>
    <% end %>
    </ul>
  </div>
<% end %>

