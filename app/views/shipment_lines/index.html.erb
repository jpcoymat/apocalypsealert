<h1>Listing shipment_lines</h1>

<table>
  <thead>
    <tr>
      <th>Shipment line number</th>
      <th>Quantity</th>
      <th>Eta</th>
      <th>Etd</th>
      <th>Origin location</th>
      <th>Destination location</th>
      <th>Order line</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipment_lines.each do |shipment_line| %>
      <tr>
        <td><%= shipment_line.shipment_line_number %></td>
        <td><%= shipment_line.quantity %></td>
        <td><%= shipment_line.eta %></td>
        <td><%= shipment_line.etd %></td>
        <td><%= shipment_line.origin_location_id %></td>
        <td><%= shipment_line.destination_location_id %></td>
        <td><%= shipment_line.order_line_id %></td>
        <td><%= link_to 'Show', shipment_line %></td>
        <td><%= link_to 'Edit', edit_shipment_line_path(shipment_line) %></td>
        <td><%= link_to 'Destroy', shipment_line, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipment line', new_shipment_line_path %>
